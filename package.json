{
    "name": "angular-web",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve -pc proxy.conf.json",
        "start:exam": "ng serve -pc proxy-custom.conf.json",
        "build": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --prod -bh /console/ -op dist/console-web",
        "build:jar": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --prod -bh /console/ -d static/",
        "test": "ng test",
        "lint": "ng lint",
        "zip": "cd dist && zip -r console-web.zip ./console-web",
        "start:aot": "ng serve -e prod -pc proxy.conf.json --aot",
        "build:aot": "ngc -p tsconfig-aot.json && rollup -c rollup-config.js",
        "build:uat": "npm run clean && npm run build && npm run zip",
        "e2e": "ng e2e",
        "clean": "rm -rf dist"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^4.0.0",
        "@angular/common": "^4.0.0",
        "@angular/compiler": "^4.0.0",
        "@angular/core": "^4.0.0",
        "@angular/forms": "^4.0.0",
        "@angular/http": "^4.0.0",
        "@angular/platform-browser": "^4.0.0",
        "@angular/platform-browser-dynamic": "^4.0.0",
        "@angular/platform-server": "^4.0.0",
        "@angular/router": "^4.0.0",
        "angular-sortablejs": "^2.5.0",
        "bootstrap": "^4.0.0",
        "classlist.js": "^1.1.20150312",
        "console-ui-ng": "~0.1.64",
        "core-js": "^2.4.1",
        "cropperjs": "1.2.2",
        "font-awesome": "^4.7.0",
        "fullpage.js": "^2.9.4",
        "intl": "^1.2.5",
        "jquery": "^3.2.1",
        "jquery-colpick": "^3.1.0",
        "jquery-toast-plugin": "^1.3.2",
        "layui-src": "^2.3.0",
        "moment": "^2.19.1",
        "ng-zorro-antd": "0.5.5",
        "object-path": "^0.11.4",
        "popper.js": "^1.11.1",
        "rxjs": "^5.1.0",
        "sortablejs": "^1.7.0",
        "web-animations-js": "^2.3.1",
        "zone.js": "^0.8.4"
    },
    "devDependencies": {
        "@angular/cli": "1.7.2",
        "@angular/compiler-cli": "^4.0.0",
        "@angular/language-service": "^4.0.0",
        "@types/jasmine": "2.5.45",
        "@types/node": "~6.0.60",
        "asciidoctor.js": "^1.5.6-preview.2",
        "codelyzer": "~3.0.1",
        "gulp": "^3.9.1",
        "gulp-clean": "^0.3.2",
        "gulp-html-beautify": "^1.0.1",
        "gulp-util": "^3.0.8",
        "gulp-watch": "^4.3.11",
        "jasmine-core": "~2.6.2",
        "jasmine-spec-reporter": "~4.1.0",
        "karma": "~1.7.0",
        "karma-chrome-launcher": "~2.1.1",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^1.2.1",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "protractor": "~5.1.2",
        "rollup": "^0.47.4",
        "rollup-plugin-commonjs": "^8.1.0",
        "rollup-plugin-node-resolve": "^3.0.0",
        "rollup-plugin-uglify": "^2.0.1",
        "through-gulp": "^0.5.0",
